<h1> List Articles </h1>
<%= link_to 'Create new user', signup_path %>

<table>
	<tr>
		<th> Name </th>
		<th> email </th>
	</tr>

	<% @user.each do |user| %>
		<tr>
			<td> <%= user.username %></td>
			<td> <%= user.email %></td>
			<td> <%= link_to 'Edit', edit_user_path(user) %></td>
			<td> <%= link_to 'Show', user_path(user) %></td>
			<td> <%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: "Sure"} %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Back to list', users_path %>